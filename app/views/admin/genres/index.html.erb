<h1>ジャンル登録/一覧</h1>
<%= form_with model: @genres, url: admin_genres_path, method: :post, local:true do |f| %>
  <h4>ジャンル名</h4>
  <%= f.text_field :name %>
  <%= f.submit '新規登録' %>
<% end %>

<% @genres.each do |genre| %>
 <div>
  <p>ジャンル：<%= genre.name %></p>
  <%= link_to "編集", edit_admin_genre_path(genre), class: 'nav-link text-dark' %>
</div>
<% end %>